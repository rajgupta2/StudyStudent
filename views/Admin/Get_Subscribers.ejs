<div class="mt-4" style="min-height: 400px;">
    <div class="table-responsive">
        <h2>Subscribers</h2>
        <% if(Subscribers!==undefined){ %>
        <table class="table table-hover  text-center" >
            <thead>
                <tr class="text-dark">
                    <th>Name</th>
                    <th>Email-ID</th>
                    <th>Subscribing Date</th>
                </tr>
            </thead>
            <tbody>
                <% Subscribers.forEach(function(e){ %>
                    <tr>
                    <td><%= e.Name%></td>
                    <td><%= e.Email%></td>
                    <td><%= e.Date%></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
        <% }else{ %>
            <h5 class="text-info">There are no subscribers yet.</h5>
        <% } %>
    </div>
</div>


