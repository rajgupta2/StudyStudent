<div class="mt-5"  style="min-height: 400px;">
    <div class="row">
        <div class="col-sm-12">
            <h2>Feedbacks</h2>
            <% if(Feedbacks.length!==0){%>
              <table class="table table-hover text-center" >
                <thead>
                    <tr class="text-dark">
                        <th>Name</th>
                        <th>Student-ID</th>
                        <th>Subject</th>
                        <th>Message</th>
                        <th>Date</th>
                    </tr>
                </thead>
                <tbody>
                  <% Feedbacks.forEach(function(e){%>
                        <tr>
                            <td><%=e.Student_Name%></td>
                            <td><%=e.Student_Id%></td>
                            <td><%=e.Feedback_Subject%></td>
                            <td><%=e.Message%></td>
                            <td><%=e.Feedback_DT%></td>
                        </tr>
                    <%});%>
                </tbody>
            </table>
            <% }else{ %>
                <h5 class="text-info">There are no feedbacks available.</h5>
            <% } %>
        </div>
    </div>
</div>