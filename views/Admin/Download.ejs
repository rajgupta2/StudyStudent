<div class="container mt-5" >
    <h2>Submitted Assignment by students</h2>
    <div class="row">
        <div class="col-sm-12">
            <table class="table table-hover">
                <thead class="table-info text-success">
                    <tr>
                        <th>Student's Name</th>
                        <th>Student's Email</th>
                        <th>Subject</th>
                        <th>Assignment</th>
                        <th>Uploaded Date</th>
                    </tr>
                </thead>
                <tbody>

                    <% if(Assignment!=null) {
                     Assignment.forEach(function(e){ %>
                        <tr>
                            <td><%=e.Student_Name%></td>
                            <td><%=e.Student_Email%></td>
                            <td><%=e.Subject%></td>
                            <td><a href="../../Content/SubmittedAssignment/<%=e.Assignment%>" download>View</a></td>
                            <td><%=e.Date%></td>
                        </tr>
                    <%});
                    }%>
                </tbody>
            </table>
        </div>
    </div>
</div>