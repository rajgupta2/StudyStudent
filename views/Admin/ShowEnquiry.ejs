<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <h1 class="text-lg-center text-success">Enquiries</h1>
            <table class="table table-hover  table-bordered text-center" >
                <thead>
                    <tr class="text-dark table-primary">
                        <th>Name</th>
                        <th>Email-ID</th>
                        <th>Phone</th>
                        <th>Query</th>
                        <th>Date</th>
                        <th>Reply</th>
                    </tr>
                </thead>
                <tbody>
                    
                       <% DBqueries.forEach(function(e){ %>
                        <tr>
                        <td><%= e.Name%></td>
                        <td><%= e.Email%></td>
                        <td><%= e.Contact%></td>
                        <td><%= e.Query%></td>
                        <td><%= e.Date%></td>
                        <td><a href="/Admin/AnswerQuery?pk=<%= e._id%>" class="btn btn-info">Answer</a></td>
                        </tr>
                        <% }); %>
                </tbody>
            </table>
        </div>
    </div>
</div>


