                <a href="#aboutus">About us</a><br />
                <a href="/Home/Contact">Contact us</a><br />
                <a href="/Home/Registration">Sign Up</a><br />
                <a href="/Home/Login">Sign In</a><br />
            </div>
            <div class="col-sm-1"></div>
            <div class="col-sm-4 pt-4">
                <h5 class="text-success">Stay updated</h5>
                <input id="Name" name="Name" placeholder="Name" type="text"
                    onchange="checkvalidation()" value=""
                    class="form-control mt-4" />
                <input id="Email" name="Email" placeholder="Email" type="email"
                    onchange="checkvalidation()" value=""
                    class="form-control mt-4" />
                <button type="button" class="btn btn-success mt-3 mb-2"
                    id="SendCode"> Verify Email</button>
                <input id="Code" name="Code"
                    placeholder="Code Sent to your email" type="hidden"
                    value="" />
                <button
                    class="g-recaptcha btn btn-danger btn-block mb-2 mt-4 d-none Subscribe_Btn"
                    data-sitekey="<%=process.env.RECAPTCHA_SITEKEY%>"
                    data-callback='onSubmit'
                    data-theme="dark">Subscribe</button>


            </div>
